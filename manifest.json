{
  "manifest_version": 2,

  "name": "Chrome Mom",
  "description": "This extension will monitor your internet usage",
  "version": "1.0",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "background" : {
    "scripts": ["background.js", "moment.js"]
  },
  "permissions": [
   "browsingData",
   "alarms",
   "background",
   "contentSettings",
   "history",
   "storage",
   "notifications",
   "webRequest",
   "webRequestBlocking",
   "tabs"
   ]
}
